<Window x:Class="AauVotingSystemP4.AddZipCodeElectionAdmin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AauVotingSystemP4"
        mc:Ignorable="d"
        Title="AddZipCodeElectionAdmin" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="zipTextBox" HorizontalAlignment="Left" Height="23" Margin="23,17,0,0" TextWrapping="Wrap" Text="Zipcode" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="nameTextBox" HorizontalAlignment="Left" Height="23" Margin="23,45,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="createButton" Content="Save" HorizontalAlignment="Left" Margin="23,73,0,0" VerticalAlignment="Top" Width="75" Click="createButton_Click"/>

    </Grid>
</Window>
