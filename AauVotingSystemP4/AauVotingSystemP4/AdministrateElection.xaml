<Window x:Class="AauVotingSystemP4.AdministrateElection"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AauVotingSystemP4"
        Loaded="Window_Loaded"
        mc:Ignorable="d"
        Title="AdministrateElection" Height="300" Width="300">
    

    <DockPanel Margin="0,0,0,0">
        <local:HeaderElectionAdmin x:Name="HeaderElectionAdmin" Grid.Row="0" UseLayoutRounding="False" DockPanel.Dock="Top" Height="41.2" />
        <StackPanel Orientation="Vertical" Margin="0,4.4,0,-74.8" RenderTransformOrigin="0.494,0.676">
            <Label VerticalAlignment="Top"  Name="SelectElection" Content="Type in the ElectionID to the election, which you want to edit"/>
            <TextBox VerticalAlignment="Top" Name="TypedInElection" Width="170"/>
            <Button VerticalAlignment="Top" HorizontalAlignment="Center" Name="SelectedElection" Content="Select election" Width="100" Click="SelectedElection_Click"/>
            <DataGrid x:Name="dataGrid1" Height="240" Width="291"/>
           
        </StackPanel>
    </DockPanel>

</Window>
