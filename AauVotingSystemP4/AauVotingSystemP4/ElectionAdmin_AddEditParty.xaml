<Window x:Class="AauVotingSystemP4.ElectionAdmin_AddEditParty"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AauVotingSystemP4"
        mc:Ignorable="d"
        Title="ElectionAdmin_AddEditParty" Height="300" Width="300">
    <Grid>
        <TextBox x:Name="partyTextBox" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="Party" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="partyButton" Content="Save" HorizontalAlignment="Left" Margin="10,38,0,0" VerticalAlignment="Top" Width="75" Click="partyButton_Click"/>

    </Grid>
</Window>
