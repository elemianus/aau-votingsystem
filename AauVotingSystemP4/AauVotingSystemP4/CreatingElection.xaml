<Page x:Class="AauVotingSystemP4.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AauVotingSystemP4"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Page1">

    <Grid>
        <TextBox x:Name="election_Type" HorizontalAlignment="Left" Height="23" Margin="76,52,0,0" TextWrapping="Wrap" Text="Election Type" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="election_Name" HorizontalAlignment="Left" Height="23" Margin="76,24,0,0" TextWrapping="Wrap" Text="Election Name" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="CreateElection" Content="Create Election" HorizontalAlignment="Left" Margin="121,167,0,0" VerticalAlignment="Top" Width="75" Click="CreateElection_Click"/>
        <DatePicker Name="start_Date" HorizontalAlignment="Left" Margin="94,80,0,0" VerticalAlignment="Top"/>
        <DatePicker Name="end_Date" HorizontalAlignment="Left" Margin="94,109,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="test_box" HorizontalAlignment="Left" Height="23" Margin="94,244,0,0" TextWrapping="Wrap" Text="TestBox" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Page>
