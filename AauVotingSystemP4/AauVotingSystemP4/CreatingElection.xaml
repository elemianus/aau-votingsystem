<Window x:Class="AauVotingSystemP4.CreatingElection"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AauVotingSystemP4"
        mc:Ignorable="d"
        Title="CreatingElection" Height="300" Width="581.749"
        WindowState="Maximized">
    <DockPanel>
        <local:HeaderUserControl x:Name="HeaderUserControl" Grid.Row="0" UseLayoutRounding="False" DockPanel.Dock="Top" Height="40"/>
        <StackPanel Margin="0,0,0,0.4" RenderTransformOrigin="0.5,0.5">
            <TextBox x:Name="election_Type" HorizontalAlignment="Left" Height="22" Margin="10,10,0,0" TextWrapping="Wrap" Text="Election Type" VerticalAlignment="Top" Width="120"/>
            <DatePicker x:Name="start_DateTimePicker" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
            <DatePicker x:Name="end_DateTimePicker" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>            
            <Button x:Name="CreateElection" Content="Create Election" HorizontalAlignment="Left" Margin="121,16,0,0" VerticalAlignment="Top" Width="75" Click="CreateElection_Click" RenderTransformOrigin="-0.393,0.812"/>
        </StackPanel>
    </DockPanel>
</Window>
