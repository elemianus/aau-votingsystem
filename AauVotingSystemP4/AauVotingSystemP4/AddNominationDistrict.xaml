<Window x:Class="AauVotingSystemP4.AddNominationDistrict"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AauVotingSystemP4"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="AddNominationDistrict">

    <Grid>
        <TextBox x:Name="nominationDistrictNameTextBox" HorizontalAlignment="Left" Height="23" Margin="25,26,0,0" TextWrapping="Wrap" Text="Name" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="nominationDistrictAmountOfVotes" HorizontalAlignment="Left" Height="23" Margin="25,66,0,0" TextWrapping="Wrap" Text="Mandates" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="saveButton" Content="Save" HorizontalAlignment="Left" Margin="25,106,0,0" VerticalAlignment="Top" Width="75" Click="saveButton_Click"/>
    </Grid>
</Window>
